{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:vue/vue3-recommended", "prettier"],
	"rules": {
		// override/add rules settings here, such as:
		// 'vue/no-unused-vars': 'error'
		"no-var": "error",
		"semi": "error",
		"no-multi-spaces": "error",
		"space-in-parens": "error",
		"no-multiple-empty-lines": "error",
		"prefer-const": "error"
	}
}
